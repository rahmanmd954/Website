{
    "name": "UT Class Tracker",
    "Author": "Md Rahman, Ranya El-Shinawy",
    "version": "1.0",
    "manifest_version": 3,
    "description": "Track UT classes by unique IDs, then scrape data from UT Registrar once user logs in.",
    "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "tabs"
    ],
    "host_permissions": [
        "*://registrar.utexas.edu/*"
    ],
    "action": {
        "default_popup": "index.html",
        "default_title": "UT Class Tracker"
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "web_accessible_resources": [
        {
            "resources": [
                "style.css",
                "list.html"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}